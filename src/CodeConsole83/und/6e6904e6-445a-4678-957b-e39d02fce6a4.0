&НаКлиенте
Функция ПараметрыПередНачаломРедактирования()
	
	Поле = Элементы.ВыбраннаяНастройка_Параметры.ТекущийЭлемент;	
	Описание = ОписаниеПараметра();
	Значение = Описание.ЗначениеПараметра;
	
	
	ИмяОткрываемойФормы = Неопределено;
	ПараметрыОткрытия = Неопределено;
	
	Если Поле.Имя = ""ВыбраннаяНастройка_ПараметрыТипПараметра"" Тогда
		
		СписокТипов = Новый СписокЗначений;
		СписокТипов.ЗагрузитьЗначения(Описание.ТипПараметра.Типы());
		
		Если СписокТипов.Количество() = 0 Тогда 
			СписокТипов.ЗагрузитьЗначения(ОписаниеДопустимыеТипы(Ложь).Типы());
		КонецЕсли;
		
		Если Описание.ЭтоТаблица Тогда
			ОТ = Новый ОписаниеТипов(""ТаблицаЗначений"");
			ВыбранныйТипПараметра = ОТ.Типы()[0];
		ИначеЕсли Описание.ВыбранныйТипПараметра.Типы().Количество() Тогда 
			ВыбранныйТипПараметра = Описание.ВыбранныйТипПараметра.Типы()[0];
		КонецЕсли;
		
		СписокТипов.ПоказатьВыборЭлемента(Новый ОписаниеОповещения(""ПослеВыбораТипа"", ЭтаФорма), ""Выберите тип"", СписокТипов.НайтиПоЗначению(ВыбранныйТипПараметра));
		
		Возврат Истина;
		
	ИначеЕсли Поле.Имя = ""ВыбраннаяНастройка_ПараметрыЗначение"" Тогда 
	
		Если Описание.ЭтоТаблица Тогда 
			
			ИмяОткрываемойФормы = ""ВнешняяОбработка.CodeConsole83.Форма.ФормаРедакторТаблицы"";
			ПараметрыОткрытия = Новый Структура(""АдресДанныхТЗ"", Описание.АдресДанных);
			
		ИначеЕсли ТипЗнч(Значение) = Тип(""Граница"") Тогда 

			ИмяОткрываемойФормы = ""ВнешняяОбработка.CodeConsole83.Форма.ФормаРедакторГраница"";
			ПараметрыОткрытия = Новый Структура(""Граница"", Значение);

		КонецЕсли;
		
	КонецЕсли;
	
	Если ИмяОткрываемойФормы <> Неопределено Тогда 
		
		Форма = ОткрытьФорму(ИмяОткрываемойФормы, ПараметрыОткрытия, Элементы.ВыбраннаяНастройка_Параметры, , , , , РежимОткрытияОкнаФормы.БлокироватьОкноВладельца);
		Форма.Инициализировать(ЭтаФорма);
		
		Возврат Истина;
		
	Иначе
		
		Возврат Ложь;
		
	КонецЕсли;

КонецФункции

