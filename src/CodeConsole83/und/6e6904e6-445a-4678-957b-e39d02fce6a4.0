	ИначеЕсли ТипЗнч(вНастройки) = Тип(""Структура"") И вНастройки.Свойство(""Дерево"") И вНастройки.Свойство(""Детали"") Тогда 
		ЗагрузитьНастройкиНаСервере_02(вНастройки);
