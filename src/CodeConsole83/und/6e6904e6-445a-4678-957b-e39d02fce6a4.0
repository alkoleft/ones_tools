&НаСервереБезКонтекста
Функция Параметры_ИзвлечьИзТекста(Знач ТекстЗапроса)
	
	мПараметры = Новый Структура;
	
	Попытка
		Запрос = Новый Запрос(ТекстЗапроса);
		ПараметрыЗапроса = Запрос.НайтиПараметры();
	Исключение
		Возврат Новый Структура(""Выполнено, Описание"", Ложь, ПодробноеПредставлениеОшибки(ИнформацияОбОшибке()));		
	КонецПопытки;
	
	Для Каждого Параметр Из ПараметрыЗапроса Цикл 
		
		Если Параметр.ТипЗначения.СодержитТип(Тип(""ТаблицаЗначений"")) Тогда 
			ОписаниеПараметра = Новый Структура(""ТипЗначения, АдресДанных"", ""ТаблицаЗначений"");
		Иначе
			ОписаниеПараметра = Новый Структура(""ТипЗначения"", Параметр.ТипЗначения);
		КонецЕсли;
		
		мПараметры.Вставить(Параметр.Имя, ОписаниеПараметра);
		
	КонецЦикла;
	
	Возврат Новый Структура(""Выполнено, Параметры"", Истина, мПараметры);
	
КонецФункции

&НаКлиенте
Процедура Параметры_ЗаполнитьНаКлиенте()
		
	ТекстЗапроса = ПолучитьТекст();
	
	Если ПустаяСтрока(ТекстЗапроса) Тогда 
		Возврат;
	КонецЕсли;
	
	Результат = Параметры_ИзвлечьИзТекста(ТекстЗапроса);
	
	Если НЕ Результат.Выполнено Тогда 
		СообщитьОбОшибке(Результат.Описание, ""При получении параметров запроса"");
		Возврат;
	КонецЕсли;
	
	ОтобразитьПараметрыВФорме(Результат.Параметры);
	
	ХранилищеНастроекПоместить(ПараметрыТекущейНастройки);
	
КонецПроцедуры

&НаКлиенте
Процедура ОтобразитьПараметрыВФорме(ПараметрыЗапроса)
	
	Для Каждого Элемент Из ПараметрыЗапроса Цикл 
		
		Параметр = Элемент.Значение;
		
		Значение = ПреобразоватьЗначениеДляОтображенияНаФорме(?(Параметр.Свойство(""Значение""), Параметр.Значение, Неопределено));
		_АдресДанных = Неопределено;
		УстановитьЗначениеПараметра(Элемент.Ключ, Значение, Параметр.ТипЗначения, Параметр.Свойство(""АдресДанных"", _АдресДанных), _АдресДанных);
		
	КонецЦикла;
	
	ЗаполнитьПредставлениеТипаДанных();
	ПриИзмененииДанныхНастройки();
	
КонецПроцедуры

&НаКлиенте
Процедура УстановитьЗначениеПараметра(Имя, Значение, ТипЗначения = Неопределено, ИспользоватьАдресДанных = Ложь, АдресДанных = Неопределено)
	
	Строки = ВыбраннаяНастройка_Параметры.НайтиСтроки(Новый Структура(""ИмяПараметра"", Имя));
	
	Если Строки.Количество() = 0 Тогда 
		Стр = ВыбраннаяНастройка_Параметры.Добавить();
		Стр.ИмяПараметра = Имя;
	Иначе
		Стр = Строки[0];
	КонецЕсли;
	
	Описание = ОписаниеПараметра(Стр);
	Описание.ЭтоТаблица = ИспользоватьАдресДанных;
	
	Если ТипЗначения <> Неопределено Тогда 
		Описание.ТипПараметра = ТипЗначения;
	КонецЕсли;
	
	Если ИспользоватьАдресДанных И ЭтоАдресВременногоХранилища(АдресДанных) Тогда 
		Описание.АдресДанных = АдресДанных;
	КонецЕсли;
	
	Описание.ЭтоСписок 	= ТипЗнч(Стр.ЗначениеПараметра) = Тип(""СписокЗначений"");
	Описание.ЗначениеПараметра 	= Стр.ЗначениеПараметра;
	
	УстановитьОписаниеПараметра(Описание);
	
КонецПроцедуры

&НаКлиенте
Процедура УстановитьЗначенияПараметров(ЗначенияПараметров) Экспорт 
	
	Для Каждого Эл из ЗначенияПараметров Цикл 
		
		УстановитьЗначениеПараметра(Эл.Ключ, Эл.Значение);
		
	КонецЦикла;
	
КонецПроцедуры

&НаКлиенте
Процедура ЗаполнитьПредставлениеТипаДанных()
	
	Для Каждого Стр Из ВыбраннаяНастройка_Параметры Цикл 
		
		Описание = ОписаниеПараметра(Стр);
		УстановитьОписаниеПараметра(Описание);
		
	КонецЦикла;
	
КонецПроцедуры

&НаКлиенте
Функция ОписаниеПараметра(Данные = Неопределено)
	
	Если Данные = Неопределено Тогда 
		Данные = Элементы.ВыбраннаяНастройка_Параметры.ТекущиеДанные;
	КонецЕсли;
	
	Если Данные = Неопределено Тогда 
		Возврат Неопределено;
	КонецЕсли;
	
	Описание = Новый Структура(""ИмяПараметра, ЗначениеПараметра, ТипПараметра, АдресДанных, ПредставлениеТипаДанных, ИспользоватьСписок, ВыбранныйТипПараметра"");
	ЗаполнитьЗначенияСвойств(Описание, Данные);
	
	Тип= ТипЗнч(Описание.ЗначениеПараметра);
	
	Описание.Вставить(""Данные"", Данные); 
	Описание.Вставить(""ЭтоСписок"", Тип = Тип(""СписокЗначений"")); 
	Описание.Вставить(""ЭтоТаблица"", ЭтоАдресВременногоХранилища(Описание.АдресДанных));
	
	Если Описание.ЭтоСписок Тогда 
		Описание.Вставить(""ОграничениеТипа"", Новый ОписаниеТипов(""СписокЗначений"")); 
	Иначе
		Описание.Вставить(""ОграничениеТипа"", ?(ЗначениеЗаполнено(Описание.ВыбранныйТипПараметра), Описание.ВыбранныйТипПараметра, ДопустимыеТипы)); 
	КонецЕсли;
	
	Возврат Описание;
	
КонецФункции

&НаКлиенте
Процедура УстановитьОписаниеПараметра(Описание)
	
	Данные = Описание.Данные;
	ОписаниеПредыдущее = ОписаниеПараметра(Данные);
	
	ЗаполнитьЗначенияСвойств(Данные, Описание);
	
	Если Описание.ЭтоТаблица <> ОписаниеПредыдущее.ЭтоТаблица Тогда 
		
		Если Описание.ЭтоТаблица Тогда 
			
			Если НЕ ЭтоАдресВременногоХранилища(Описание.АдресДанных) Тогда 
				Данные.АдресДанных = ПоместитьВоВременноеХранилище(Неопределено, УникальныйИдентификатор);
			КонецЕсли;
			
			Данные.ЗначениеПараметра = Неопределено;
			Данные.ВыбранныйТипПараметра = Неопределено;			
			Данные.ТипПараметра = Неопределено;
			
		Иначе
			
			Если ЭтоАдресВременногоХранилища(Данные.АдресДанных) Тогда 
				УдалитьИзВременногоХранилища(Данные.АдресДанных);
				Данные.АдресДанных = Неопределено;
			КонецЕсли;
			
		КонецЕсли;
		
	КонецЕсли;
	
	Если Описание.ЭтоСписок <> ОписаниеПредыдущее.ЭтоСписок Тогда 
		
		Если Описание.ЭтоСписок И ТипЗнч(Данные.ЗначениеПараметра) <> Тип(""СписокЗначений"") Тогда 
			
			Данные.ЗначениеПараметра = Новый СписокЗначений;
			
			Если ЗначениеЗаполнено(Описание.ЗначениеПараметра) Тогда 
				Данные.ЗначениеПараметра.Добавить(Описание.ЗначениеПараметра);
			КонецЕсли;
			
		ИначеЕсли НЕ Описание.ЭтоСписок И ТипЗнч(Данные.ЗначениеПараметра) = Тип(""СписокЗначений"") Тогда 
			
			Данные.ЗначениеПараметра = ?(Описание.ЗначениеПараметра.Количество(), Описание.ЗначениеПараметра[0].Значение, Неопределено);
			
		КонецЕсли;
		
	КонецЕсли;
	
	Данные.ПредставлениеТипаДанных = ?(Описание.ЭтоТаблица, ""Таблица значений"", ТипЗнч(Данные.ЗначениеПараметра));
	Данные.ИспользоватьСписок = Описание.ЭтоСписок;
	
	Если НЕ Описание.ЭтоТаблица И НЕ ЗначениеЗаполнено(Описание.ВыбранныйТипПараметра) Тогда
		
		мТипы = Новый Массив;
		Если Описание.ЭтоСписок Тогда 
			Данные.ВыбранныйТипПараметра = Описание.ЗначениеПараметра.ТипЗначения;
		ИначеЕсли Описание.ЗначениеПараметра <> Неопределено Тогда 
			мТипы.Добавить(ТипЗнч(Описание.ЗначениеПараметра));				
			Данные.ВыбранныйТипПараметра = Новый ОписаниеТипов(мТипы);
		ИначеЕсли Описание.ТипПараметра.Типы().Количество() Тогда 
			мТипы.Добавить(Описание.ТипПараметра.Типы()[0]);
			Данные.ВыбранныйТипПараметра = Новый ОписаниеТипов(мТипы);
		КонецЕсли;
		
	КонецЕсли;
	
	Если Описание.ЭтоСписок Тогда 
		Данные.ЗначениеПараметра.ТипЗначения = Описание.ВыбранныйТипПараметра;
	ИначеЕсли НЕ Описание.ЭтоСписок И НЕ Описание.ЭтоТаблица Тогда 
		Данные.ЗначениеПараметра = Данные.ВыбранныйТипПараметра.ПривестиЗначение(Данные.ЗначениеПараметра);
	КонецЕсли;
	
КонецПроцедуры

